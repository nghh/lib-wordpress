{
    "name": "nghh/wordpress",
    "type": "library",
    "description": "Libraray of PHP Utilities and Helper Classes for Wordpress",
    "keywords": [
        "php", "helper", "utilities"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Jan Reiland",
        "email": "moin@nordgestalten.com",
        "homepage": "https://github.com/nghh"
    }],
    "repositories": [{
        "type": "path",
        "url": "~/Development/composer/nghh/helper"
    }],
    "require": {
        "php": ">=7.1",
        "nghh/helper": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nghh\\Lib\\Wordpress\\": "src/"
        },
        "files": [
            "src/functions_include.php"
        ]
    }
}